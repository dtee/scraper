  <DataType>
    <DataTypeID>-2</DataTypeID>
    <DataTypeName>NaturalNumberDataRefiner</DataTypeName>
    <DataTypeCode>Imports System
Imports Microsoft.VisualBasic
Imports System.Text.RegularExpressions
Imports DataRefinerAsm

Public Class NaturalNumberDataRefiner
   Inherits DataRefiner

   Public Overrides Function Refine(ByVal Text As String) As Object
      Dim m As Match
      m = Regex.Match(Text, "\d+")

      If m.Success Then
         Return Double.Parse(m.Value)
      Else
         Return Nothing
      End If
   End Function
End Class
</DataTypeCode>
  </DataType>
  <DataType>
    <DataTypeID>-3</DataTypeID>
    <DataTypeName>MoneyDataRefiner</DataTypeName>
    <DataTypeCode>Imports System
Imports Microsoft.VisualBasic
Imports System.Text.RegularExpressions
Imports DataRefinerAsm

Public Class MoneyDataRefiner
   Inherits DataRefiner

   Public Overrides Function Refine(ByVal Text As String) As Object
      Dim m As Match = Regex.Match(Text, "\$.*?(\d+\.\d+)")

      If m.Success Then
         m = Regex.Match(m.Value, "\d+\.\d+")

         Return Double.Parse(m.Value)
      End If

      Return Nothing
   End Function
End Class
</DataTypeCode>
  </DataType>
